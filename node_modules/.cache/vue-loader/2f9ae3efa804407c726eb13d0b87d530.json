 table-hover table-striped\" >\r\n            <thead>\r\n                <tr>\r\n                    <th>NISN</th>\r\n                    <th>Nama</th>\r\n                    <th>Kelas</th>\r\n                    <th>Jurusan</th>\r\n                    <th>nominal</th>\r\n                    <th>Aksi</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"tun in tunggakan.data\" :key=\"tun.id_transaksi\">\r\n                    <td>{{tun.nisn}}</td>\r\n                    <td v-for=\"siswa in listsiswa.data\" :key=\"siswa.nisn\" v-bind:value=\"siswa.nisn\">{{siswa.nama}}</td>\r\n                    <td v-for=\"kelas in listkelas.data\" :key=\"kelas.id_kelas\" v-bind:value=\"kelas.id_kelas\">{{kelas.nama_kelas}}</td>\r\n                    <td v-for=\"kelas in listkelas.data\" :key=\"kelas.id_kelas\" v-bind:value=\"kelas.id_kelas\">{{kelas.jurusan}}</td>\r\n                    <td v-for=\"spp in listspp.data\" :key=\"spp.id_spp\" v-bind:value=\"spp.id_spp\">{{spp.nominal}}</td>\r\n                    <td>\r\n                        <router-link class=\"btn btn-success\" :to=\"{path:'/tunggakan/bayar'+tun.id_transaksi}\"><i class='bx bx-dollar-circle'></i></router-link>\r\n                        &nbsp;\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"Tunggakan\",\r\n    data(){\r\n        return{\r\n            tunggakan:[],\r\n            listsiswa:[],\r\n            listkelas:[],\r\n            listspp:[],\r\n            nisn:'',\r\n            message:'',\r\n            error:false,\r\n        }\r\n    },\r\n    methods:{\r\n        tsiswa:function(){\r\n            var option={\r\n                headers:{\r\n                    'Authorization':'bearer '+localStorage.getItem(\"token\")\r\n                    }\r\n            }\r\n            this.axios.get(\"http://localhost/latihan_migrasi/public/api/siswa\", option).then((result)=>{\r\n                this.listsiswa=result.data\r\n            })\r\n        },\r\n        tkelas:function(){\r\n            var option={\r\n                headers:{\r\n                    'Authorization':'bearer '+localStorage.getItem(\"token\")\r\n                    }\r\n            }\r\n            this.axios.get(\"http://localhost/latihan_migrasi/public/api/kelas\", option).then((result)=>{\r\n                this.listkelas=result.data\r\n            })\r\n        },\r\n        tspp:function(){\r\n            var option={\r\n                headers:{\r\n                    'Authorization':'bearer '+localStorage.getItem(\"token\")\r\n                    }\r\n            }\r\n            this.axios.get(\"http://localhost/latihan_migrasi/public/api/spp\", option).then((result)=>{\r\n                this.listspp=result.data\r\n            })\r\n        },\r\n        // ttunggakan:function(){\r\n        //     var option={\r\n        //         headers:{\r\n        //             'Authorization':'bearer '+localStorage.getItem(\"token\")\r\n        //             }\r\n        //     }\r\n        //     this.axios.get(\"http://localhost/latihan_migrasi/public/api/kurangbayar/\"+ this.nisn, option).then((result)=>{\r\n        //         this.tunggakan=result.data\r\n        //     })\r\n        //     this.axios.get(\"http://localhost/latihan_migrasi/public/api/kurangbayarr/\"+ this.nisn, option).then((result)=>{\r\n        //         this.tunggakan=result.data\r\n        //     })\r\n        //     this.axios.get(\"http://localhost/latihan_migrasi/public/api/kurangbayarrr/\"+ this.nisn, option).then((result)=>{\r\n        //         this.tunggakan=result.data\r\n        //     })\r\n        // }\r\n\r\n    },\r\n    mounted(){\r\n        this.tkelas()\r\n        this.tsiswa()\r\n        this.ttunggakan()\r\n        this.tspp()\r\n    }\r\n}\r\n</script>"]}]}