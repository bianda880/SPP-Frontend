,EAToB,CAapB;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACH,KAvCG;AAwCJ,IAAA,UAAU,EAAC,sBAAW;AAAA;;AAClB,UAAI,MAAK,GAAI;AACT,QAAA,OAAO,EAAC;AACJ,2BAAgB,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADxB;AADC,OAAb;AAMA,WAAK,KAAL,CAAW,GAAX,CAAe,mDAAf,EAAoE,MAApE,EAA4E,IAA5E,CAAiF,UAAC,MAAD,EAAY;AACzF;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,MAAM,CAAC,IAAtB;AACH,OAHD;AAIH,KAnDG;AAqDJ,IAAA,QAAQ,EAAC,oBAAW;AAAA;;AAChB,UAAI,MAAK,GAAI;AACT,QAAA,OAAO,EAAC;AACJ,2BAAgB,YAAY,YAAY,CAAC,OAAb,CAAqB,OAArB;AADxB;AADC,OAAb;AAKA,WAAK,KAAL,CAAW,GAAX,CAAe,mDAAf,EAAoE,MAApE,EAA4E,IAA5E,CAAiF,UAAC,MAAD,EAAY;AACzF;AACA,QAAA,MAAI,CAAC,KAAL,GAAa,MAAM,CAAC,IAApB;AACH,OAHD;AAIH,KA/DG;AAgEJ,IAAA,KAAK,EAAC,iBAAU;AACZ,MAAA,MAAM,CAAC,KAAP;AACJ;AAlEI,GAbG;AAiFX,EAAA,OAjFW,qBAiFF;AACL,SAAK,aAAL;AACA,SAAK,UAAL;AACA,SAAK,QAAL;AACJ;AArFW,CAAf","sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <br>\r\n        <div style=\"padding: 25px; padding-bottom: 0;\">\r\n            <input type=\"text\" name=\"search\" v-model=\"search\" class=\"form-control\" placeholder=\"Search...\" \r\n            @keyup.enter=\"cari()\" @input=\"cari()\" autocomplete=\"off\">\r\n        </div>\r\n\r\n        <table class=\"table table-hover table-striped data\" >\r\n            <thead>\r\n                <tr>\r\n                    <th>ID Pembayaran</th>\r\n                    <th>Nama Petugas</th>\r\n                    <th>Nama Siswa</th>\r\n                    <th>Tanggal Bayar</th>\r\n                    <th>Bulan Bayar</th>\r\n                    <th>Tahun Bayar</th>\r\n                    <th>Status</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"pem in pembayaran\" :key=\"pem.id_pembayaran\">\r\n                    <td>{{pem.id_pembayaran}}</td>\r\n                    <td>\r\n                       <span v-for=\"pet in petugas.filter((pet) => pem.id_petugas === pet.id_petugas)\" :key=\"pet.id_petugas\">\r\n                           {{pet.name}}\r\n                        </span> \r\n                    </td>\r\n                    <td>\r\n                        <span v-for=\"sis in siswa.filter((sis) => pem.nisn === sis.nisn)\" :key=\"sis.nisn\">\r\n                            {{sis.nama}}\r\n                        </span>\r\n                    </td>\r\n                    <td>{{pem.tgl_bayar}}</td>\r\n                    <td>{{pem.bulan_bayar}}</td>\r\n                    <td>{{pem.tahun_bayar}}</td>\r\n                    <td>{{pem.status}}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n        <button class=\"btn btn-primary\" style=\"float:right\" @click=\"cetak()\" v-if=\"level !='siswa'\"><i class='bx bx-printer'></i></button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"Pembayaran\",\r\n    data(){\r\n        return{\r\n            pembayaran:[],\r\n            petugas:[],\r\n            siswa:[],\r\n            search:'',\r\n            message:'',\r\n            style_msg:'',\r\n            error:false,\r\n        }\r\n    },\r\n    methods:{\r\n        cari:function() {\r\n                var option = {\r\n                    headers:{\r\n                        'Authorization':'bearer ' + localStorage.getItem(\"token\")\r\n                    }\r\n                }\r\n                var data_cari = \"\"\r\n                if(this.search == \"\") {\r\n                    data_cari = \"\"\r\n                } else {\r\n                    data_cari = \"/\" + this.search\r\n                }\r\n                this.axios.get(\"http://localhost/latihan_migrasi/public/api/pembayarana\" + data_cari, option).then((result) => {\r\n                    console.log(result)\r\n                    this.pembayaran = result.data\r\n                })\r\n        },\r\n        tm_pembayaran:function(){\r\n            var option={\r\n                headers:{\r\n                    'Authorization':'bearer ' + localStorage.getItem(\"token\")\r\n                    }\r\n            }\r\n\r\n            // var data = localStorage.getItem('nisn');\r\n\r\n            this.axios.get(\"http://localhost/latihan_migrasi/public/api/pembayaran\", option).then((result)=>{\r\n                // console.log(result)\r\n                this.pembayaran=result.data\r\n            })\r\n            // this.axios.get(\"http://localhost/latihan_migrasi/public/api/pembayarans\", option).then((result)=>{\r\n            //     // console.log(result)\r\n            //     this.pembayaran=result.data\r\n            // })\r\n            // this.axios.get(\"http://localhost/latihan_migrasi/public/api/pembayaranss/\"+ data, option).then((result)=>{\r\n            //     console.log(result)\r\n            //     this.pembayaran=result.data\r\n            // })\r\n        },\r\n        getpetugas:function() {\r\n            var option = {\r\n                headers:{\r\n                    'Authorization':'bearer ' + localStorage.getItem(\"token\")\r\n                }\r\n            }\r\n\r\n            this.axios.get(\"http://localhost/latihan_migrasi/public/api/users\", option).then((result) => {\r\n                // console.log(result)\r\n                this.petugas = result.data\r\n            })\r\n        },\r\n\r\n        getsiswa:function() {\r\n            var option = {\r\n                headers:{\r\n                    'Authorization':'bearer ' + localStorage.getItem(\"token\")\r\n                }\r\n            }\r\n            this.axios.get(\"http://localhost/latihan_migrasi/public/api/siswa\", option).then((result) => {\r\n                // console.log(result)\r\n                this.siswa = result.data\r\n            })\r\n        },\r\n        cetak:function(){\r\n            window.print()\r\n        }    \r\n    },\r\n    mounted(){\r\n        this.tm_pembayaran()\r\n        this.getpetugas()\r\n        this.getsiswa()\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}