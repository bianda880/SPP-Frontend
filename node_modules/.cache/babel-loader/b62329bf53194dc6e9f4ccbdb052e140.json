ion(id){\r\n            var option={\r\n                headers:{\r\n                    'Authorization':'bearer '+localStorage.getItem(\"token\")\r\n                    }\r\n            }\r\n            if(confirm('yakin?')) {\r\n                this.axios.delete(\"http://localhost/latihan_migrasi/public/api/delete_user/\"+id, option).then((result)=>{\r\n                console.log(result)\r\n                this.tm_petugas()\r\n                })\r\n            }\r\n        }\r\n    },\r\n    mounted(){\r\n        this.tm_petugas()\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}