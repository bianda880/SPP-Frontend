ame:\"TambahSpp\",\r\n    data(){\r\n        return{\r\n            angkatan:'',\r\n            tahun:'',\r\n            nominal:'',\r\n            style_msg:'',\r\n            message:'',\r\n            error:false,\r\n        }\r\n    },\r\n    methods:{\r\n        simpanspp:function(){\r\n            var dataspp = {\r\n                angkatan:this.angkatan,\r\n                tahun:this.tahun,\r\n                nominal:this.nominal,\r\n            }\r\n            var option={\r\n                headers:{\r\n                    'Authorization':'bearer '+localStorage.getItem(\"token\")\r\n                    }\r\n            }\r\n            this.axios.post(\"http://localhost/latihan_migrasi/public/api/insert_spp\", dataspp, option).then((result)=>{\r\n                console.log(result)\r\n                if(result.request.status==200){\r\n                    if (result.data.status==true){\r\n                        this.error=false\r\n                        this.message=result.data.message\r\n                        this.style_msg=\"alert alert-success\"\r\n                    }else{\r\n                        this.error=true\r\n                        this.message=result.data.message\r\n                        this.style_msg=\"alert alert-danger\"\r\n                    }\r\n                    \r\n                    setTimeout(()=>{\r\n                    this.$router.push('/spp')\r\n                    }, 2000)\r\n                }else{\r\n                    this.error=true\r\n                    this.style_msg=\"alert alert-danger\"\r\n                    this.message=result.data.status                    \r\n                }\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>"]}]}